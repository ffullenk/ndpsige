<h1>Listing cajeros</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @cajeros.each do |cajero| %>
      <tr>
        <td><%= cajero.nombre %></td>
        <td><%= link_to 'Show', cajero %></td>
        <td><%= link_to 'Edit', edit_cajero_path(cajero) %></td>
        <td><%= link_to 'Destroy', cajero, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cajero', new_cajero_path %>
